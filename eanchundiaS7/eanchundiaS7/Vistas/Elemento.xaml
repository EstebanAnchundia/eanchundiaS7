<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="eanchundiaS7.Vistas.Elemento">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="ACTUALIZAR - ELIMINAR" FontSize="20"/>
            <Entry x:Name="txtNombre" Placeholder="Nombre"></Entry>
            <Entry x:Name="txtUsuario" Placeholder="Usuario"></Entry>
            <Entry x:Name="txtContraseña" Placeholder="Copntraseña"></Entry>

            <Button x:Name="btnActualizar" Text="ACTUALIZAR" Clicked="btnActualizar_Clicked"></Button>
            <Button x:Name="btnEliminar" Text="ELIMINAR" Clicked="btnEliminar_Clicked"></Button>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>